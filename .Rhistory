bootstrap <- function(a) {
a.mean <- mean(a)
n <- 999
alpha <- 0.1
a.b <- a.mean
for (i in 1:n) {
b <- sample(a, replace = T)
a.b <- c(a.b, mean(b))
}
a.b.sorted <- sort(a.b)
left <- a.b.sorted[floor((n+1)*alpha/2)]
right <- a.b.sorted[ceiling((n+1)*(1-alpha/2))]
return(c(left, right))
}
